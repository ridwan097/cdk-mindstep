{
  "name": "cdk-mindstep",
  "version": "0.1.0",
  "bin": {
    "cdk-mindstep": "bin/cdk-mindstep.js"
  },
  "main": "app.ts",
  "scripts": {
    "start:local": "npm run cdk:synth && npm run start:sam",
    "start:sam": "sam local start-api -t ./cdk.out/CdkMindstepStack.template.json",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:synth": "cdk synth",
    "local": "npx tsc && npx aws-cdk synth && sam local start-api -t ./cdk.out/CdkMindstepStack.template.json",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "install-all": "npm install && cd src && npm install && cd .."
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.132",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "aws-cdk": "2.149.0",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3",
    "aws-lambda": "^1.0.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.149.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "mongoose": "^8.5.4",
    "source-map-support": "^0.5.21"
  }
}
